{
  "version": 2,
  "builds": [
    {
      "src": "web_app.py",
      "use": "@streamlit/vercel-build@1.0.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "STREAMLIT_SERVER_PORT": "8501",
    "STREAMLIT_SERVER_ADDRESS": "0.0.0.0"
  }
}
